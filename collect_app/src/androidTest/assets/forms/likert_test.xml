        <select1 appearance="likert" ref="/likert_test/likert_widget">
            <label>Likert Widget</label>
            <hint>Likert type widget (happy case)</hint>
            <item>
                <label>0</label>
                <value>choice_0</value>
            </item>
            <item>
                <label>1</label>
                <value>choice_1</value>
            </item>
            <item>
                <label>2</label>
                <value>choice_2</value>
            </item>
            <item>
                <label>3</label>
                <value>choice_3</value>
            </item>
            <item>
                <label>4</label>
                <value>choice_4</value>
            </item>
        </select1>
        <select1 appearance="likert" ref="/likert_test/likert_image_widget">
            <label>Likert Image Widget</label>
            <hint>Likert type widget with images (happy case)</hint>
            <item>
