            </item>
            <item>
                <label
                    ref="jr:itext('/likert_test/likert_image_widget_error2/strongly_agree:label')" />
                <value>strongly_agree</value>
            </item>
        </select1>
        <select1 appearance="likert" ref="/likert_test/likert_error_missing_text">
            <label>Likert Missing text Error</label>
            <hint>When there is a missing Text</hint>
            <item>
                <label>0</label>
                <value>choice_0</value>
            </item>
            <item>
                <label></label>
                <value>choice_1</value>
            </item>
            <item>
                <label>2</label>
                <value>choice_2</value>
            </item>
            <item>
                <label>3</label>
                <value>choice_3</value>
            </item>
        </select1>
        <group appearance="field-list" ref="/likert_test/cascading_select">
            <label>Cascading likert</label>
            <select1 appearance="likert" ref="/likert_test/cascading_select/source1">
                <label>Level1</label>
                <item>
                    <label>A</label>
                    <value>a</value>
                </item>
                <item>
                    <label>B</label>
                    <value>b</value>
                </item>
                <item>
                    <label>C</label>
                    <value>c</value>
                </item>
            </select1>
            <select1 appearance="likert" ref="/likert_test/cascading_select/target1">
                <label>Level2</label>
                <itemset nodeset="instance('level2')/root/item[level1 =  /likert_test/cascading_select/source1 ]">
                    <value ref="name"/>
                    <label ref="jr:itext(itextId)"/>
                </itemset>
            </select1>
            <select1 appearance="likert" ref="/likert_test/cascading_select/target1-1">
                <label>Level3</label>
                <itemset nodeset="instance('level3')/root/item[level2 =  /likert_test/cascading_select/target1 ]">
                    <value ref="name"/>
                    <label ref="jr:itext(itextId)"/>
                </itemset>
            </select1>
        </group>
