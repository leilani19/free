            </item>
            <item>
                <label ref="jr:itext('/likert_test/likert_image_widget/agree:label')" />
                <value>agree</value>
            </item>
            <item>
                <label ref="jr:itext('/likert_test/likert_image_widget/strongly_agree:label')" />
                <value>strongly_agree</value>
            </item>
        </select1>
        <select1 appearance="likert" ref="/likert_test/likert_widget_error">
            <label>Likert Widget Error</label>
            <hint>Insufficient text provided</hint>
            <item>
                <label>0</label>
                <value>choice_0</value>
            </item>
            <item>
                <label>1</label>
                <value>choice_1</value>
            </item>
            <item>
                <label>2</label>
                <value>choice_2</value>
            </item>
            <item>
                <label>3</label>
                <value>choice_3</value>
            </item>
        </select1>
        <select1 appearance="likert" ref="/likert_test/likert_image_widget_error">
            <label>Likert Image Widget Error</label>
            <hint>Insufficient images provided</hint>
            <item>
                <label
                    ref="jr:itext('/likert_test/likert_image_widget_error/strongly_disagree:label')" />
                <value>strongly_disagree</value>
            </item>
            <item>
                <label ref="jr:itext('/likert_test/likert_image_widget_error/disagree:label')" />
                <value>disagree</value>
            </item>
            <item>
                <label>Neutral</label>
                <value>neutral</value>
            </item>
            <item>
                <label ref="jr:itext('/likert_test/likert_image_widget_error/agree:label')" />
