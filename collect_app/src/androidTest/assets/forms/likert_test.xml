            </item>
        </select1>
        <select1 appearance="likert" ref="/likert_test/likert_image_widget_error2">
            <label>Likert Image Widget Error2</label>
            <hint>Image cannot be found</hint>
            <item>
                <label
                    ref="jr:itext('/likert_test/likert_image_widget_error2/strongly_disagree:label')" />
                <value>strongly_disagree</value>
            </item>
            <item>
                <label ref="jr:itext('/likert_test/likert_image_widget_error2/disagree:label')" />
                <value>disagree</value>
            </item>
            <item>
                <label ref="jr:itext('/likert_test/likert_image_widget_error2/neutral:label')" />
